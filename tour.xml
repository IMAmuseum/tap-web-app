    <tourml:Title xml:lang="en"><![CDATA[Visitors Pavilion]]></tourml:Title>
    <tourml:Description xml:lang="en"><![CDATA[<p>Hear from the architect and engineer of the Visitors Pavilion.</p>
]]></tourml:Description>
    <tourml:AssetRef tourml:id="asset-image-13" tourml:usage="image"/>
