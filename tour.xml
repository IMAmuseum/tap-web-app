  <tourml:Stop tourml:id="stop-12" tourml:view="stop_group">
    <tourml:Title xml:lang="en"><![CDATA[The Ruth Lilly Visitor's Pavilion]]></tourml:Title>
    <tourml:Description xml:lang="en"><![CDATA[<p>Designed by Marlon Blackwell Architects, the Ruth Lilly Visitors Pavilion serves as the cornerstone of the park and is one of the region’s signature architectural landmarks.</p>
]]></tourml:Description>
    <tourml:AssetRef tourml:id="asset-geo-12-0" tourml:usage="geo"/>
